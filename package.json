{
    "name": "vue3-snippets-plus",
    "displayName": "vue3 snippets plus",
    "description": "vue3 code snippets plus for development",
    "version": "0.0.1",
    "publisher": "Owen.dev",
    "engines": {
        "vscode": "^1.75.0"
    },
    "icon": "assets/logo.png",
    "keywords": [
        "vue",
        "snippets",
        "vue3"
    ],
    "repository": {
        "type": "git",
        "url": ""
    },
    "bugs": {
        "url": "https://github.com/wejectchen/vue3-snippets-for-vscode/issues",
        "email": "461662596@qq.com"
    },
    "license": "MIT",
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "vue",
                "aliases": [
                    "Vue",
                    "vue",
                    "vue2",
                    "vue3"
                ],
                "extensions": [
                    ".vue"
                ]
            }
        ],
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "vue",
                "path": "./snippets/vue.json"
            },
            {
                "language": "vue-html",
                "path": "./snippets/vue-html.json"
            },
            {
                "language": "html",
                "path": "./snippets/vue-html.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/vue-script.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/vue-script.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            }
        ]
    }
}
